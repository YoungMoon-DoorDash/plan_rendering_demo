<form style="width:100%;margin-top:10px;" class="text-start" action="/experiment/import" method="post">
    <table style='margin-top:10px;width:100%;'>
        <tr>
            <td><b>{{title}}</b></td>
            <td style='padding-left:16px;text-align:right;' nowrap>
                <input type="submit" class='btn btn-outline-primary btn-sm' value="Import">
                <a type="button" class='btn btn-outline-primary btn-sm' href="/experiment">Cancel</a>
            </td>
        </tr>
    </table>

    <div class="form-group">
        <textarea class="form-control" id="values" name="values" rows="32" style="font-size:small;width:100%;" required></textarea>
    </div>
    <br>
</form>

<script>
    document.getElementById('name').focus()
</script>

