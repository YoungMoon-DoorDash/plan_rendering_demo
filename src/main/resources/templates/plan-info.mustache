{{> header}}
<script>
    function on_delete(id) {
        if (confirm("Are you want to remove this plan information!")) {
            location.href = "/plan/delete?id=" + id
        }
    }
</script>
{{> bodyheader}}
{{{banner}}}

<table style='margin-top:10px; width:100%;'>
    <tr>
        <td nowrap><b>{{title}}</b></td>
        <td style='padding-left:16px;text-align:right;' nowrap>
            {{#plan}}
                <button type="button" class='btn btn-outline-primary btn-sm' onclick="on_delete({{id}})">Remove</button>
            {{/plan}}
            <a href='/plan/import' class='btn btn-outline-primary btn-sm'>Import</a>
        </td>
    </tr>
</table>

{{#plan}}
    <table class="table table-striped table-bordered btn-sm" style="width:100%;">
        <tr><td scope="row" style="text-align:right;font-weight:bold;" nowrap>ID &nbsp;</td><td>&nbsp; {{id}}</td></tr>
        <tr><td scope="row" style="text-align:right;font-weight:bold;" nowrap>Plan Type &nbsp;</td><td>&nbsp; {{plan_type}}</td></tr>
        <tr><td scope="row" style="text-align:right;font-weight:bold;" nowrap>Integration Name &nbsp;</td><td>&nbsp; {{integration_name}}</td></tr>
        <tr><td scope="row" style="text-align:right;font-weight:bold;" nowrap>Priority &nbsp;</td><td>&nbsp; {{priority}}</td></tr>
        <tr><td scope="row" style="text-align:right;font-weight:bold;" nowrap>Fee &nbsp;</td><td>&nbsp; {{fee}}</td></tr>
        <tr><td scope="row" style="text-align:right;font-weight:bold;" nowrap>Currency &nbsp;</td><td>&nbsp; {{currency}}</td></tr>
        <tr><td scope="row" style="text-align:right;font-weight:bold;" nowrap>Trial Plans &nbsp;</td><td>&nbsp;{{{trials}}}</td></tr>
        <tr><td scope="row" style="text-align:right;font-weight:bold;" nowrap>Created At &nbsp;</td><td>&nbsp; {{created_at}}</td></tr>
